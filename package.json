{
  "name": "express-to-lambda",
  "version": "1.0.1",
  "main": "./lib/cjs/index.js",
  "module": "./lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsc -p tsconfig.json && npm run tsc && npm run copy-handler",
    "clean": "rm -rf ./lib",
    "tsc": "tsc -p tsconfig-cjs.json && ./fixup",
    "copy-handler": "cp ./src/utils/handler.js ./lib/utils"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmin220304/express-to-lambda.git"
  },
  "type": "module",
  "keywords": [
    "express to lambda",
    "express lambda"
  ],
  "author": "cosmin aftanase",
  "license": "MIT",
  "description": "",
  "files": [
    "lib/**/*"
  ]
}
